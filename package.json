{
  "name": "@micimize/jupyterlab-wikify",
  "version": "0.0.1",
  "description": "Markdown extensions and link+stub auto-resolution (Fork of @agoose77/jupyterlab-markup)",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/micimize/jupyterlab-wikify",
  "bugs": {
    "url": "https://github.com/micimize/jupyterlab-wikify/issues"
  },
  "license": "BSD-3-Clause",
  "author": "micimize",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/micimize/jupyterlab-wikify.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
