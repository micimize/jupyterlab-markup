  "dependencies": {
    "@gerhobbelt/markdown-it-wikilinks": "^1.1.1-8",
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/codemirror": "^2.0.2",
    "@jupyterlab/fileeditor": "^2.0.2",
    "@jupyterlab/markdownviewer": "^2.0.2",
    "@jupyterlab/rendermime": "^2.0.2",
    "@jupyterlab/rendermime-interfaces": "^2.0.1",
    "@lumino/messaging": "^1.3.3",
    "change-case": "^4.1.1",
    "markdown-it": "^10.0.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-diagrams": "^0.1.2",
    "markdown-it-footnote": "^3.0.2",
    "slugify": "^1.4.5"
  },
  "devDependencies": {
    "@jupyterlab/docmanager": "^2.2.2",
    "@types/codemirror": "^0.0.74",
    "@types/markdown-it": "^0.0.9",
    "@types/markdown-it-container": "^2.0.2",
    "rimraf": "~3.0.0",
    "typescript": "~3.7.3"
  },
  "jupyterlab": {
    "extension": true
